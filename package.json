{
  "name": "count-snooze",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "concurrently -p \"[{name}]\" --names \"build,test\" -c \"blue.bold,yellow.bold\"  \"npm run dev:watch\"  \"npm run test:watch\"",
    "dev:watch": "npm run build:hmr && (run-p wss build:watch)",
    "test": "vitest run",
    "test:watch": "vitest watch --coverage.skipFull",
    "lint": "tsc --noEmit && ESLINT_USE_FLAT_CONFIG=true eslint . --format stylish && prettier . --check",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier . --write"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.18",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.18",
    "@mui/x-date-pickers": "^7.5.1",
    "chart.js": "^4.4.3",
    "dayjs": "^1.11.11",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "use-debounce": "^10.0.0",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.3.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "react-remove-attr": "^0.0.6",
    "rollup": "^4.18.0",
    "sass": "^1.77.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "ws": "^8.17.0"
  }
}
