{
  "name": "count-snooze",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "npm@10.9.0",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "concurrently -p \"[{name}]\" --names \"build,test\" -c \"blue.bold,yellow.bold\"  \"npm run dev:watch\"  \"npm run test:watch\"",
    "dev:watch": "npm run build:hmr && (run-p wss build:watch)",
    "test": "vitest run",
    "test:watch": "vitest watch --coverage.skipFull",
    "lint": "tsc --noEmit && ESLINT_USE_FLAT_CONFIG=true eslint . --format stylish && prettier . --check",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier . --write"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.2",
    "@mui/lab": "^6.0.0-beta.10",
    "@mui/material": "^6.1.2",
    "chart.js": "^4.4.4",
    "dayjs": "^1.11.13",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "structurajs": "^0.12.6",
    "use-debounce": "^10.0.3",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@rollup/plugin-typescript": "^12.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/chrome": "^0.0.272",
    "@types/eslint": "^9.6.1",
    "@types/node": "~22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.1",
    "chokidar": "^4.0.1",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup": "^4.22.5",
    "sass": "^1.79.4",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.1",
    "vitest-fail-on-console": "^0.7.1",
    "ws": "^8.18.0"
  }
}
