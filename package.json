{
  "name": "tab-count-snooze",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "npm@10.9.0",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev": "concurrently -p \"[{name}]\" --names \"build,test\" -c \"blue.bold,yellow.bold\"  \"npm run dev:watch\"  \"npm run test:watch\"",
    "dev:watch": "vite build -w --mode development",
    "test": "vitest run",
    "test:watch": "vitest watch --coverage.skipFull",
    "lint": "tsc --noEmit && ESLINT_USE_FLAT_CONFIG=true eslint . --format stylish && prettier . --check",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier . --write"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.9",
    "@mui/lab": "^6.0.0-beta.17",
    "@mui/material": "^6.1.9",
    "chart.js": "^4.4.6",
    "dayjs": "^1.11.13",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "structurajs": "^0.12.6",
    "use-debounce": "^10.0.4",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/chrome": "^0.0.287",
    "@types/eslint": "^9.6.1",
    "@types/node": "~22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.6",
    "chokidar": "^4.0.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup": "^4.27.4",
    "sass": "^1.81.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.6",
    "vitest-fail-on-console": "^0.7.1",
    "ws": "^8.18.0"
  }
}
